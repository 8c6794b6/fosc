#!/bin/sh
#|-*- mode:lisp -*-|#
#| Script to run FOSC tests.
exec ros -Q -- $0 "$@"
|#
(ros:ensure-asdf)
(ql:quickload :fosc)

(defpackage :ros.script.run-test
  (:use :cl))

(in-package :ros.script.run-test)

(defun main (&rest argv)
  (declare (ignore argv))
  (asdf:test-system :fosc))

